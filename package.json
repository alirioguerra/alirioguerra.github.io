{
  "name": "alirioguerra",
  "version": "1.0.0",
  "homepage": "https://alirioguerra.github.io/",
  "description": "portfólio de desenvolvimento",
  "author": "Alírio Guerra",
  "license": "MIT",
  "scripts": {
    "build": "postcss input.css -o style.css --watch",
    "build:prod": "postcss input.css -o style.css --env production",
    "dev": "postcss input.css -o style.css --watch",
    "preview": "live-server --port=3000 --open=/index.html",
    "start": "concurrently \"npm run dev\" \"npm run preview\"",
    "build:js": "vite build",
    "build:js:watch": "vite build --watch"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "gh-pages": "^5.0.0",
    "imagemin": "^8.0.1",
    "imagemin-webp": "^8.0.0",
    "live-server": "^1.2.2",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "tailwindcss": "^3.4.0",
    "vite": "^7.0.6"
  },
  "dependencies": {
  }
}
